{
  "version": 2,
  "builds": [
    {
      "src": "api_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api_server.py"
    }
  ],
  "env": {
    "PYTHONPATH": ".",
    "MODEL_NAME": "u2netp"
  },
  "functions": {
    "api_server.py": {
      "maxDuration": 10
    }
  },
  "build": {
    "env": {
      "PYTHONPATH": "."
    }
  },
  "installCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "public": false
}